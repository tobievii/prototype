{
  "name": "prototype",
  "version": "5.0.33",
  "description": "Typescrypt based IoT framework for general purpose remote monitoring/control.",
  "main": "src/main.js",
  "scripts": {
    "start": "node ./build/main.js",
    "test": "mocha --exit ./build/tests/tests.js",
    "tests": "node ./build/tests.js",
    "build": "tsc --lib es2015 ./src/main.ts ./src/tests/tests.ts --outDir ./build",
    "buildwatch": "tsc --sourcemap --w --lib es2015 ./src/main.ts ./src/tests/tests.ts --outDir ./build",
    "build-clean": "rmdir build /s /q && tsc",
    "debug": "cd build && nodemon --inspect main.js"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:IoT-nxt/prototype.git"
  },
  "keywords": [
    "iot",
    "iotnxt",
    "rpi",
    "node",
    "ts"
  ],
  "author": "Rouan van der Ende",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.1.7",
    "@types/enzyme": "^3.1.16",
    "@types/express": "^4.16.0",
    "@types/geoip-lite": "^1.1.29",
    "@types/leaflet": "^1.4.1",
    "@types/lodash": "^4.14.116",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.11.3",
    "@types/react-leaflet": "^1.1.6",
    "@types/sinon": "^7.0.5",
    "chai": "^4.2.0",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "css-loader": "^2.1.0",
    "discord.js": "^11.4.2",
    "enzyme": "^3.8.0",
    "express": "^4.16.3",
    "geoip-lite": "^1.3.2",
    "leaflet": "^1.4.0",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mocha-jenkins-reporter": "^0.4.1",
    "mongojs": "^2.6.0",
    "mqtt": "^2.18.8",
    "nodemailer": "^4.6.8",
    "nodemon": "^1.18.9",
    "random-string": "^0.2.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-leaflet": "^2.2.0",
    "react-modal": "^3.8.1",
    "request": "^2.88.0",
    "sinon": "^7.2.3",
    "socket.io": "^2.1.1",
    "source-map-support": "^0.5.9",
    "typescript": "^3.1.3",
    "vm2": "^3.6.3"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2"
  }
}
