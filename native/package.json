{
  "name": "PROTOTYP3",
  "version": "1.0.0",
  "private": true,
  "author": "IoT.nxt",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "npm run env:set -- dev && npm start -- -c",
    "start:prod": "npm run env:set -- prod && npm start -- -c",
    "env:set": "./scripts/environment/set-env.sh",
    "build": "tsc -p ./tsconfig.json",
    "clean": "rimraf ./dist",
    "lint": "tslint -p ./tsconfig.json",
    "lint:fix": "npm run lint -- --fix",
    "ci:install": "npm ci",
    "ci:lint": "npm run lint",
    "ci:build": "npm run env:set -- prod && npm run build",
    "conventional-changelog": "conventional-changelog",
    "version:changelog": "npm run conventional-changelog -- -i ./CHANGELOG.md -s"
  },
  "dependencies": {
    "@eva-design/eva": "^1.0.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/react-native-fontawesome": "^0.1.0",
    "expo": "^33.0.6",
    "expo-analytics": "^1.0.11",
    "expo-camera": "^5.0.1",
    "expo-constants": "^5.0.1",
    "expo-media-library": "^5.0.1",
    "expo-permissions": "^5.0.1",
    "react": "^16.8.3",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-keyboard-aware-scroll-view": "^0.8.0",
    "react-native-maps": "^0.24.2",
    "react-native-svg": "^9.5.3",
    "react-native-ui-kitten": "^4.1.0",
    "react-navigation": "^3.11.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/runtime": "^7.5.4",
    "@types/react": "^16.8.23",
    "@types/react-native": "^0.57.65",
    "@types/react-navigation": "^3.0.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "conventional-changelog-cli": "^2.0.21",
    "husky": "^1.1.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
}
